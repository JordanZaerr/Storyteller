{
  "buildOptions": {
    "debugType": "portable",
    "emitEntryPoint": false,
    "compile": [
      "**/*.cs",
      "../CommonAssemblyInfo.cs"
    ],
    "embed": ["../../client/public/**/*.css", "../ST/batch-bundle.js"]
  },
  "version": "3.0.0-alpha-*",
  "title": "Storyteller",
  "description": "Automated Acceptance Testing and Technical Documentation Authoring",
  "language": "en-US",
  "authors": [
    "Jeremy D. Miller"
  ],
  "packOptions": {
    "projectUrl": "http://storyteller.github.io",
    "tags": [
      "testing"
    ],
    "licenseUrl": "https://github.com/storyteller/storyteller/raw/master/LICENSE.TXT",
    "repository": {
      "type": "git",
      "url": "git://github.com/storyteller/storyteller"
    }
  },
  "dependencies": {
    "HtmlTags.AspNetCore": "4.1.0",
    "Baseline": "1.1.0",
    "System.Reflection.TypeExtensions": "4.1.0"
  },
  "frameworks": {
    "net46": {
      "frameworkAssemblies": {
        "System.Data": "",
        "System.Reflection": ""
      },
      "dependencies": {
        "System.Data.Common": "4.1.0"

      }
    },
    "netstandard1.6": {
      "dependencies": {
        "System.Data.Common": "4.1.0",
        "System.Xml.XmlDocument": "4.0.1",
        "System.Net.Sockets": "4.1.0",
        "System.Diagnostics.TextWriterTraceListener": "4.0.0",
        "System.Threading.Thread": "4.0.0",
        "System.Threading.Timer": "4.0.1",
        "System.Threading": "4.0.11" 
      }
    }
  }
}
